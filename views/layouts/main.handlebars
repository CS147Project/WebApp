<!doctype html>

<html>
  <head>
    <title>Team Workout App - Home</title> 
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- link CSS Files -->
    <link href="css/bootstrap.css" rel="stylesheet">
    <link href="css/index.css" rel="stylesheet">

    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
      <![endif]-->        
  </head>
  <body>
    <div id="container" class="container">
      <!-- HEADER -->
      <div id="header" class="header">
        <div id="logo">
          <h1> TeamWorkout </h1>
        </div>
        <div id="accountMenu">
          <div class="dropdown">
            <button class="btn dropdown-toggle sr-only" type="button" id="dropdownMenu1" data-toggle="dropdown">
              Account
              <span class="caret"></span>
            </button>
            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
              <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Action</a></li>
              <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Another action</a></li>
              <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Something else here</a></li>
              <li role="presentation" class="divider"></li>
              <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Separated link</a></li>
            </ul>
          </div>


          <a href="logout">
            <button type="button" class="btn btn-default btn">
              <span class="glyphicon glyphicon-off glyphicons-lg"></span> Logout
            </button>
          </a>
          <a href="settings" id="settings_btn" class="btn btn-lg"> <span class="glyphicon glyphicon-cog glyphicons-lg"></span> Settings </a>
        </div>
      </div> <!-- END OF HEADER -->
      <!-- CONTENT -->
      <div id="content" class="content clearfix">

        {{{body}}}

      </div> <!-- END OF CONTENT -->

      <!-- FOOTER -->
      <div id="footer" class="footer navbar-fixed-bottom">
        <!-- GENERAL NAVIGATION -->
        <div id="genNav" >
          <ul class="navbar">
            <li><a href="home" id="home-btn" class="btn"> <img src="img/home_icon.png" alt="Home"> Home </a></li>
            <li><a href="messages" id="message-btn" class="btn btn-lg"> <img src="img/message_icon.png" alt="Messages"> Messages </a></li>
            <li><a href="workouts" id="workout-btn" class="btn"> <img src="img/workout_icon.png" alt="Workouts"> Workouts </a></li>
            <li><a href="analytics" id="analytics_btn" class="btn inactive"> <img src="img/analytics_icon.png" alt="Analytics"> Analytics </a></li>
          </ul>
        </div>
      </div> 
      <!-- END OF FOOTER -->
    </div>

    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="/js/bootstrap.js"></script>
    <script src="/js/general.js"></script>
    <script src="//use.edgefonts.net/alex-brush:n4:all.js"></script> 

